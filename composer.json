{
  "name": "davidhirtz/yii2-tenant",
  "description": "Multi-tenant support for Yii 2 framework",
  "license": "proprietary",
  "type": "yii2-extension",
  "homepage": "https://www.github.com/davidhirtz/yii2-vite",
  "authors": [
    {
      "name": "David Hirtz",
      "homepage": "https://www.davidhirtz.com/"
    }
  ],
  "require": {
    "davidhirtz/yii2-skeleton": "^2.3"
  },
  "require-dev": {
    "rector/rector": "^1.0",
    "symfony/var-dumper": "^6.3",
    "symplify/easy-coding-standard": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\tenant\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "rector": "rector"
  }
}
